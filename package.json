{
  "name": "gpw",
  "version": "0.2.0",
  "description": "generate password",
  "main": "index.js",
  "watch": {
    "lint": "src/index.ts"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:ts": "tsc -p .",
    "build:js": "browserify build/index.js > index.js",
    "lint": "tslint -p .",
    "fix": "tslint -p . --fix",
    "pretest": "run-s build lint",
    "test": "node index.js",
    "preserve": "npm run build",
    "serve": "http-server -o -s -c-1",
    "watch": "run-p watch:*",
    "watch:npm": "npm-watch",
    "watch:ts": "tsc -p . --watch",
    "prerelease": "npm run build",
    "release": "mkdir -p dist && cp index.* dist/ && surge dist gpw.surge.sh",
    "dev": "run-p watch serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/gpw.git"
  },
  "keywords": [
    "generate",
    "password"
  ],
  "author": "Beeno Tung",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/beenotung/gpw/issues"
  },
  "homepage": "https://github.com/beenotung/gpw#readme",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "typestub-qrcode": "^0.1.0"
  },
  "devDependencies": {
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@types/crypto-js": "^3.1.37",
    "@types/node": "^8.0.51",
    "browserify": "^14.5.0",
    "codelyzer": "^4.0.1",
    "http-server": "^0.10.0",
    "npm-run-all": "^4.1.2",
    "npm-watch": "^0.3.0",
    "rxjs": "^5.5.5",
    "surge": "^0.19.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.6.1",
    "typestub-horizon-client": "^0.1.8"
  }
}
